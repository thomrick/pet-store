{
  "name": "@thomrick/nest-recipes",
  "version": "0.0.0",
  "description": "A Nest application that use many recipes about Nest framework",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts",
    "start:dev:watch": "nodemon -w src -e ts --exec 'npm run -s start:dev'",
    "test": "jest",
    "test:watch": "nodemon -w src -e ts --exec 'npm run -s test'"
  },
  "author": "ThomRick",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/platform-express": "^9.2.1",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.8",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
